{
  "name": "uptime-monitor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "scheduler:dev": "cd workers/scheduler && wrangler dev",
    "scheduler:deploy": "cd workers/scheduler && wrangler deploy",
    "load-test:deploy": "cd workers/load-test && wrangler deploy",
    "d1:create": "wrangler d1 create uptime-monitor-db",
    "d1:list": "wrangler d1 list",
    "d1:migrate": "wrangler d1 execute uptime-monitor-db --file=./migrations/schema.sql",
    "d1:migrate:remote": "wrangler d1 execute uptime-monitor-db --remote --file=./migrations/schema.sql",
    "d1:remove-projects": "wrangler d1 execute uptime-monitor-db --remote --file=./migrations/006_remove_projects.sql",
    "d1:add-response-body": "wrangler d1 execute uptime-monitor-db --remote --file=./migrations/007_add_response_body_to_cron_runs.sql",
    "d1:drop-monitor-checks": "wrangler d1 execute uptime-monitor-db --remote --file=./migrations/005_drop_monitor_checks_table.sql",
    "d1:query": "wrangler d1 execute uptime-monitor-db --command",
    "d1:query:remote": "wrangler d1 execute uptime-monitor-db --remote --command"
  },
  "dependencies": {
